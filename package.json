{
  "name": "authentication-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npm run start:db\" \"ts-node-dev src/server.ts\"",
    "start:db": "docker-compose up -d auth-service-db",
    "stop:db": "docker-compose down"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "bcrypt": "^5.1.1",
    "fastify": "^4.29.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^18.0.0",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}